{
  "name": "@collaborne/kubernetes-aws-metadata-service",
  "version": "0.0.1",
  "description": "Service that adds AWS-specific annotations to pods",
  "scripts": {
    "build": "tsc",
    "install": "npm-warn-shrinkwrap",
    "lint": "tslint -c tslint.json \"src/**/*.ts\" \"test/**/*.ts\"",
    "postinstall": "check-node-version --package",
    "test": "echo 'No tests defined' >&2",
    "start": "node -r source-map-support/register ${_NODE_ARGS} build/index.js"
  },
  "engines": {
    "node": ">=12.18.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Collaborne/kubernetes-aws-metadata-service.git"
  },
  "author": "Andreas Kohn <andreas.kohn@gmail.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/Collaborne/kubernetes-aws-metadata-service/issues"
  },
  "homepage": "https://github.com/Collaborne/kubernetes-aws-metadata-service#readme",
  "devDependencies": {
    "@types/node": "^12.12.50",
    "ts-node": "^8.10.2",
    "tsconfig-collaborne": "^1.3.0",
    "tslint": "^6.1.2",
    "tslint-config-collaborne": "^1.0.0",
    "typescript": "^3.9.6"
  },
  "dependencies": {
    "@types/express": "^4.17.7",
    "@types/yargs": "^15.0.5",
    "auto-kubernetes-client": "^0.6.2",
    "aws-sdk": "^2.713.0",
    "check-node-version": "^4.0.3",
    "express": "^4.17.1",
    "express-prom-bundle": "^6.0.0",
    "log4js": "^6.3.0",
    "npm-warn-shrinkwrap": "^1.0.2",
    "prom-client": "^12.0.0",
    "source-map-support": "^0.5.19",
    "yargs": "^15.4.1"
  }
}
